   issue = {5},
   journal = {Nature Reviews Earth & Environment 2023 4:5},
   keywords = {Climate sciences,Hydrology,Sustainability},
   month = {5},
   pages = {319-332},
   publisher = {Nature Publishing Group},
   title = {Big Data in Earth system science and progress towards a digital twin},
   volume = {4},
   url = {https://www.nature.com/articles/s43017-023-00409-w},
   year = {2023},
}
