@article{edmonds2019using,
   abstract = {Scientific modelling can make things worse, as in the case of the North Atlantic Cod Fisheries Collapse. Some of these failures have been attributed to the simplicity of the models used compared to what they are trying to model. MultiAgent-Based Simulation (MABS) pushes the boundaries of what can be simulated, prompting many to assume that it can usefully inform policy, even in the face of complexity. That said, MABS also brings with it new difficulties and potential confusions. This paper surveys some of the pitfalls that can arise when MABS analysts try to do this. Researchers who claim (or imply) that MABS can reliably predict are criticised in particular. However, an alternative is suggested – that of using MABS for a kind of uncertainty analysis – identifying some of the possible ways a policy can go wrong (or indeed go right). A fisheries example is given. This alternative may widen, rather than narrow, the range of evidence and possibilities that are considered, which could enrich the policy-making process. We call this Reflexive Possibilistic Modelling.},
   author = {Bruce Edmonds and Lia ní Aodha},
   doi = {10.1007/978-3-030-22270-3_1/FIGURES/6},
   isbn = {9783030222697},
   issn = {16113349},
   journal = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
   pages = {1-16},
   publisher = {Springer Verlag},
   title = {Using Agent-Based Modelling to Inform Policy – What Could Possibly Go Wrong?},
   volume = {11463 LNAI},
   url = {https://link.springer.com/chapter/10.1007/978-3-030-22270-3_1},
   year = {2019},
}
